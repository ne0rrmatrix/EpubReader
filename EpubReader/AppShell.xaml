<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="EpubReader.AppShell"
    x:Name="Page"
    Shell.FlyoutBehavior="Disabled">
    <TabBar>
        <Tab Title="Library" Icon="library.png">
            <ShellContent Route="MainPage" ContentTemplate="{DataTemplate LibraryPage}" />
        </Tab>
        <Tab Title="Recent" Icon="book_open.png">
            <ShellContent ContentTemplate="{DataTemplate RecentBooksPage}" />
        </Tab>
    </TabBar>
</Shell>

